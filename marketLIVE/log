    <title>Log</title>

    <script type="text/javascript">
      $(document).ready(function() {
        var cluster_batch = new Array();
        $('td[cluster_id]').each(function(index) {
          var id = $(this).attr('cluster_id');
          var ql = $(this).attr('template_ql');
          if ( $.grep(cluster_batch, function(id, ql) { return function(n) { return (n.cluster_id == id && n.template_ql == ql); } }(id, ql)) == false ) {
            cluster_batch.push({cluster_id: id, template_ql: ql});
          }
        });
        $.postJSON('/marketLIVE/get_item_visuals_batch', {clusters: cluster_batch}, 
                      function(response, text_status, jqXHR) {
                        $('td[cluster_id]').each(function(index) {
                          var id = $(this).attr('cluster_id');
                          var ql = $(this).attr('template_ql');
                          //this.innerHTML = response[id][ql].name;
                          $(this).html(response[id][ql].name);
                          
                        });
                      }, ajaxError); 
        
        $('.scroll-pane-arrows').jScrollPane({showArrows: true, horizontalGutter: 10});
        $('tr').each(function() { jQuery('#cash', this).html(Globalize.format(parseInt(jQuery('#cash', this).text()), "n0"));
                                  jQuery('#count', this).html(Globalize.format(parseInt(jQuery('#count', this).text()), "n0"));
                                });
      });
      
    </script>      
