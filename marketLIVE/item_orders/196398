      $(document).ready(function() {
        if (typeof AnarchyOnline == "undefined") {
          AnarchyOnline = new Object();
          AnarchyOnline.AcceptLanguage = [];
          AnarchyOnline.IsInGame = false;
        }
        Globalize.culture(AnarchyOnline.AcceptLanguage);
